#include<iostream>
#include<cstring>

char* delspace(char* a){
	char* b = a;
	int c = strlen(b);
	for(int i = 0;i < c;i++){
		if((b[i] == ' ') && (b[i+1] == ' ')){
			for(int j = i;j < c; j++){
				b[j] = b[j+1];
				i--;
			}
		}
		if((i == 0 )&& (b[i] == ' ')){
			for(int j = 0;j < c;j++){
				b[j] = b[j+1];
				i--;
			}
		}
	}
	return(b);
}
int main(){
	char a[100];
	gets(a);
	char* b = delspace(a);
	puts(b);
	return(0);
}
