#include <iostream>
#include <string.h>

using namespace std;

char ** sort(char ** array, int N) {
    for(int i = 0 ; i < N; i++){
        for(int j = i+1; j < N; j++)
            if(strcmp(array[i], array[j]) > 0)
            {
                char* s = array[i];
                array[i] = array[j];
                array[j] = s;
            }
    }
    return array;
}

int main() {
    int N;
    cout << "Input array size: " << endl;
    cin >> N;
    char **array = new char *[N];
    
    for(int i = 0; i<N;i++) {
        array[i] = new char[100];
        cin >> array[i];
    }
    char **newArray = sort(array, N);;
    for(int i = 0; i<N; i++) {
        for(int j = 0; j<N; j++) {
        cout << newArray[i][j];
        }
        cout << " ";
    }
    cout << endl;
    for(int i = 0; i < N; i++) {
        delete []array;
        delete []newArray;
    }
    delete []array;
    delete []newArray;

    return 0;
}
