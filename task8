#include <iostream>

using namespace std;

int main() {
    int N = 3;
    int M = 3;
    int matrix[N][M];

    for(int i = 0; i < N; i++) {
        for(int j = 0; j < M; j++) {
            cin >> matrix[i][j];
        }
    }
    cout<<endl;
    for(int i = 0; i < N; i++) {
        for(int j = M-1; j >= 0; j--) {
            cout<<matrix[j][i] << " ";
        }
        cout<<endl;
    }

    return 0;
}
